{
  "name": "antd-react-template",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "start": "cross-env ANALYZE=1 umi dev",
    "dev": "umi dev --port=8080",
    "build": "cross-env ANALYZE=1 umi build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "fix": "yarn lint -- --fix",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ],
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@emotion/css": "^11.7.1",
    "ahooks": "^3.3.10",
    "antd": "^4.18.2",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "decimal.js": "^10.3.1",
    "eslint": "^8.6.0",
    "eslint-config-react-app": "^7.0.0",
    "less-vars-to-js": "^1.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "query-string": "^7.1.1",
    "react": "17.X",
    "react-dom": "17.x",
    "react-router-dom": "^6.2.1",
    "recoil": "^0.5.2",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
