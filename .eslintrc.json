{
    "extends": ["react-app"],
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": ["node_modules", "src"],
          "extensions": [".js", ".jsx", ".ts", ".tsx", ".d.ts", ".png"]
        }
      }
    },
    "rules": {
      "import/extensions": ["error", "never",{ "png": "always" }],
      "import/no-named-as-default": "off",
      "default-param-last": "off",
      "camelcase": ["off", { "properties": "never" }]
    },
  
    "ignorePatterns": ["*.d.ts"]
  }